   "source": [
    "## Dropping the row records that don't want to be contacted in the Do_Not_Contact column"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 96,
   "id": "954719a5",
   "metadata": {},
   "outputs": [],
   "source": [
    "for x in df.index:\n",
    "    if df.loc[x, 'Do_Not_Contact'] == 'Y':\n",
    "       df.drop(x, inplace = True)"
   ]
  },
